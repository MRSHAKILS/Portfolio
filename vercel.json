{
  "version": 2,
  "name": "portfolio-app",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "functions": {
    "frontend/src/**/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
